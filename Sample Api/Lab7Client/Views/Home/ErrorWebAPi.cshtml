@model IEnumerable<Lab7Client.Models.ErrorWebAPiModel>

@{
    ViewData["Title"] = "ErrorWebAPi";
}

<div>
<h1 class="text-center text-primary">Web API Errors</h1>

</div>


<table class="table table-hover table-striped table-bordered">
    <thead class="table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Element)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Message)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Element)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
        </tr>
}
    </tbody>
</table>
